<p id="notice"><%= notice %></p>

<h1>Listing Community Memberships</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Community</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @community_memberships.each do |community_membership| %>
      <tr>
        <td><%= community_membership.user_id %></td>
        <td><%= community_membership.community_id %></td>
        <td><%= link_to 'Show', community_membership %></td>
        <td><%= link_to 'Edit', edit_community_membership_path(community_membership) %></td>
        <td><%= link_to 'Destroy', community_membership, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Community membership', new_community_membership_path %>
